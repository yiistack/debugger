{
    "name": "yiistack/debugger",
    "type": "library",
    "description": "A debugger for Yii 3",
    "keywords": [
        "debugger"
    ],
    "homepage": "https://github.com/yiistack/debugger",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/yiistack/debugger/issues?state=open",
        "source": "https://github.com/yiistack/debugger"
    },
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/infection-static-analysis-plugin": "^1.6",
        "spatie/phpunit-watcher": "^1.23",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Yiistack\\Debugger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiistack\\Debugger\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "scripts": {
        "test": "phpunit --testdox --no-interaction",
        "test-watch": "phpunit-watcher watch"
    },
    "config": {
        "sort-packages": true
    }
}
